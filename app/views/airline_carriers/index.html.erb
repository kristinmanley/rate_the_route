<div class="page-header">
  <h1>
    Airline carriers
    <a href="/airline_carriers/new" class="btn btn-lg btn-success">New Airline carrier</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Actions</th>
      </tr>

      <% @airline_carriers.each do |airline_carrier| %>
      <tr>
        <td>
          <a href="/airline_carriers/<%= airline_carrier.id %>" class="btn btn-primary">Show</a>
          <a href="/airline_carriers/<%= airline_carrier.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_airline_carrier/<%= airline_carrier.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
    <%= paginate @airline_carriers, theme: 'twitter-bootstrap-4' %>
  </div>
</div>
